<?xml version="1.0"?>
<typelistextension
  xmlns="http://guidewire.com/typelists"
  desc="Type of the document for access-restriction purposes"
  name="DocumentSecurityType">
  <typecode
    code="unrestricted"
    desc="Document which does not require access restriction"
    name="Unrestricted document"/>
  <typecode
    code="internalonly"
    desc="Document which should not be viewed by people outside the company"
    name="Internal Only document"/>
  <typecode
    code="sensitive"
    desc="Document which is sensitive in nature"
    name="Sensitive document"/>
</typelistextension>