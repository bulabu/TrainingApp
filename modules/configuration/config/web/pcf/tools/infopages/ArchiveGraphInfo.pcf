<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../../../pcf.xsd">
  <Page
    canEdit="false"
    canVisit="com.guidewire.pl.system.dependency.PLDependencies.getDomainGraphSupport() != null"
    id="ArchiveGraphInfo"
    title="displaykey.Web.InternalTools.InfoPages.ArchivingInfo">
    <Screen>
      <Variable
        initialValue="com.guidewire.pl.domain.archiving.DomainGraphIssues.validateNothingOutsideOfDomainGraphShouldPointToDomainGraph()"
        name="warnings1"/>
      <Variable
        initialValue="com.guidewire.pl.domain.archiving.DomainGraphIssues.validateNullLinksCannotMakeNodeUnreachable()"
        name="warnings2"/>
      <DetailViewPanel>
        <InputColumn>
          <ButtonInput
            action="gw.api.web.tools.ArchiveGraphInfoPage.download()"
            download="true"
            id="download"
            labelAbove="true"
            value="displaykey.Button.Download"/>
        </InputColumn>
      </DetailViewPanel>
      <CardViewPanel>
        <Card
          id="Graphs"
          title="displaykey.Web.InternalTools.InfoPages.ArchivingInfo.Graphs">
          <DetailViewPanel>
            <InputColumn>
              <TextAreaInput
                id="instructions1"
                labelAbove="true"
                numCols="50"
                numRows="6"
                value="displaykey.Web.InternalTools.InfoPages.ArchivingInfo.DotFormatInstructions"/>
              <TextAreaInput
                id="instructions2"
                labelAbove="true"
                numCols="150"
                numRows="6"
                value="displaykey.Web.InternalTools.InfoPages.ArchivingInfo.GraphLegend"/>
            </InputColumn>
          </DetailViewPanel>
          <PanelRef>
            <TitleBar
              title="displaykey.Web.InternalTools.InfoPages.ArchivingInfo.DomainGraphInDotFormat"/>
            <DetailViewPanel
              editable="false">
              <InputColumn>
                <TextAreaInput
                  id="graph1"
                  labelAbove="true"
                  numCols="150"
                  numRows="30"
                  value="com.guidewire.pl.domain.archiving.DomainGraphIssues.testOutputDomainGraphInDotFormat()"/>
              </InputColumn>
            </DetailViewPanel>
          </PanelRef>
        </Card>
        <Card
          id="Warnings"
          title="displaykey.Web.InternalTools.InfoPages.ArchivingInfo.Warnings">
          <PanelRef
            visible="!warnings1.IsEmpty">
            <TitleBar
              title="displaykey.Web.InternalTools.InfoPages.ArchivingInfo.ValidateNoEntityOutsideOfDomainGraphShouldPointToDomainGraph"/>
            <Toolbar/>
            <ListViewPanel>
              <RowIterator
                editable="false"
                elementName="warning1"
                id="warningLV1"
                value="warnings1">
                <Row>
                  <Cell
                    id="warningMessage1"
                    value="warning1"
                    width="1000"
                    wrap="true"/>
                </Row>
              </RowIterator>
            </ListViewPanel>
          </PanelRef>
          <PanelRef
            visible="!warnings2.IsEmpty">
            <TitleBar
              title="displaykey.Web.InternalTools.InfoPages.ArchivingInfo.ValidateNullLinksCannotMakeNodeUnreachable"/>
            <Toolbar/>
            <ListViewPanel>
              <RowIterator
                editable="false"
                elementName="warning2"
                id="warningLV2"
                value="warnings2">
                <Row>
                  <Cell
                    id="warningMessage2"
                    value="warning2"
                    width="1000"
                    wrap="true"/>
                </Row>
              </RowIterator>
            </ListViewPanel>
          </PanelRef>
        </Card>
      </CardViewPanel>
      <PanelRef
        visible="warnings1.IsEmpty and warnings2.IsEmpty">
        <TitleBar
          title="displaykey.Web.InternalTools.InfoPages.ArchivingInfo.WarningsNone"/>
        <DetailViewPanel>
          <InputColumn/>
        </DetailViewPanel>
      </PanelRef>
    </Screen>
  </Page>
</PCF>