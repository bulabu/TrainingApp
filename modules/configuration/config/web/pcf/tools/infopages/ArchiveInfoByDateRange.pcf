<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../../../pcf.xsd">
  <Page
    autosaveable="false"
    canEdit="true"
    id="ArchiveInfoByDateRange"
    parent="ArchiveInfo()"
    startInEditMode="true"
    title="displaykey.Web.ServerTools.ArchiveInfo.ArchiveInfoByDay">
    <LocationEntryPoint
      signature="archiveInfoByDateRange(archiveCountSummaryByDatamodelVersion : gw.api.archiving.ArchiveCountSummaryByDatamodelVersion, beginTime : java.util.Date, endTime : java.util.Date)"/>
    <Variable
      name="archiveCountSummaryByDatamodelVersion"
      type="gw.api.archiving.ArchiveCountSummaryByDatamodelVersion"/>
    <Variable
      name="beginTime"
      type="java.util.Date"/>
    <Variable
      name="endTime"
      type="java.util.Date"/>
    <Variable
      initialValue="new gw.api.archiving.ArchiveCountSummaryByDateRange(archiveCountSummaryByDatamodelVersion, beginTime, endTime)"
      name="archiveCountSummaryByDateRange"
      type="gw.api.archiving.ArchiveCountSummaryByDateRange"/>
    <Screen
      editable="true">
      <TitleBar
        title="displaykey.Web.ServerTools.ArchiveInfo.ArchiveInfoFor(archiveCountSummaryByDatamodelVersion.VersionString)"/>
      <Toolbar/>
      <DetailViewPanel
        editable="true">
        <InputColumn>
          <ButtonInput
            action="ArchiveInfo.go()"
            id="BackButton"
            labelAbove="true"
            value="displaykey.Web.ServerTools.ArchiveInfo.Back(displaykey.Web.InternalTools.InfoPages.ArchiveInfo)"/>
        </InputColumn>
        <InputColumn>
          <DateInput
            dateFormat="short"
            editable="true"
            focusOnStartEditing="true"
            id="BeginTime"
            label="displaykey.Web.ServerTools.ArchiveInfo.BeginTime"
            required="true"
            timeFormat="short"
            value="beginTime"/>
        </InputColumn>
        <InputColumn>
          <DateInput
            dateFormat="short"
            editable="true"
            id="EndTime"
            label="displaykey.Web.ServerTools.ArchiveInfo.EndTime"
            required="true"
            timeFormat="short"
            value="endTime"/>
        </InputColumn>
        <InputColumn>
          <ButtonInput
            action="archiveCountSummaryByDateRange.BeginTime = beginTime; archiveCountSummaryByDateRange.EndTime = endTime"
            id="Search"
            labelAbove="true"
            value="displaykey.Web.ServerTools.ArchiveInfo.View"/>
        </InputColumn>
      </DetailViewPanel>
      <PanelSet
        available="archiveCountSummaryByDateRange.canQuery()"
        editable="false"
        visible="archiveCountSummaryByDateRange.canQuery()">
        <DetailViewPanel
          editable="false">
          <InputColumn>
            <TextInput
              id="Archived"
              label="displaykey.Web.ServerTools.ArchiveInfo.Archived"
              value="archiveCountSummaryByDateRange.ArchivedCount"/>
          </InputColumn>
          <InputColumn>
            <TextInput
              id="ExcludedBecauseOfRules"
              label="displaykey.Web.ServerTools.ArchiveInfo.ExcludedBecauseOfRules"
              value="archiveCountSummaryByDateRange.getExcludedBecauseOfRulesCount()"/>
          </InputColumn>
          <InputColumn>
            <ButtonInput
              action="archiveCountSummaryByDateRange.resetExcludedBecauseOfRulesFlag()"
              available="archiveCountSummaryByDateRange.getExcludedBecauseOfRulesCount() &gt; 0"
              hideIfDisabled="false"
              id="ResetExcludedBecauseOfRules"
              labelAbove="true"
              value="displaykey.Web.ServerTools.ArchiveInfo.Reset"/>
          </InputColumn>
          <InputColumn>
            <TextInput
              id="ExcludedBecauseOfFailure"
              label="displaykey.Web.ServerTools.ArchiveInfo.ExcludedBecauseOfFailure"
              value="archiveCountSummaryByDateRange.getExcludedBecauseOfFailureCount()"/>
          </InputColumn>
          <InputColumn>
            <ButtonInput
              action="archiveCountSummaryByDateRange.resetExcludedBecauseOfFailureFlag()"
              available="archiveCountSummaryByDateRange.getExcludedBecauseOfFailureCount() &gt; 0"
              hideIfDisabled="false"
              id="ResetExcludedBecauseOfFailure"
              labelAbove="true"
              value="displaykey.Web.ServerTools.ArchiveInfo.Reset"/>
          </InputColumn>
        </DetailViewPanel>
        <PanelDivider/>
        <PanelRef
          available="archiveCountSummaryByDateRange.canQuery()"
          def="ArchiveExcludeDetailsCV(archiveCountSummaryByDateRange)"
          editable="false"
          visible="archiveCountSummaryByDateRange.canQuery()"/>
      </PanelSet>
    </Screen>
  </Page>
</PCF>