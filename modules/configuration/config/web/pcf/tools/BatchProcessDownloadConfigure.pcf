<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../../pcf.xsd">
  <Page
    autosaveable="false"
    canEdit="true"
    canVisit="true"
    id="BatchProcessDownloadConfigure"
    parent="BatchProcessInfo()"
    startInEditMode="true"
    title="displaykey.Web.InternalTools.BatchProcessInfo.Download.Select.Date">
    <LocationEntryPoint
      signature="BatchProcessDownloadConfigure(batchProcessType : BatchProcessType)"/>
    <Variable
      name="batchProcessType"
      type="BatchProcessType"/>
    <Screen
      editable="true"
      id="BatchProcessDownloadConfigureScreen">
      <Variable
        initialValue="new com.guidewire.pl.system.batchprocessing.BatchInfoHelper(batchProcessType)"
        name="BatchInfoHelper"/>
      <DetailViewPanel>
        <InputColumn>
          <DateInput
            dateFormat="short"
            editable="true"
            focusOnStartEditing="true"
            id="StartDate"
            label="displaykey.Web.InternalTools.BatchProcessInfo.Download.Start.Date"
            required="true"
            value="BatchInfoHelper.StartDate"/>
          <DateInput
            dateFormat="short"
            editable="true"
            focusOnStartEditing="false"
            id="StopDate"
            label="displaykey.Web.InternalTools.BatchProcessInfo.Download.End.Date"
            required="true"
            value="BatchInfoHelper.EndDate"/>
          <ButtonInput
            action="BatchInfoHelper.download()"
            download="true"
            id="download"
            value="displaykey.Button.Complete.Download"/>
          <ButtonInput
            action="BatchProcessInfo.go()"
            id="BackButton"
            value="displaykey.Web.InternalTools.BatchProcessInfo.Download.Return.BatchProcessInfo"/>
        </InputColumn>
      </DetailViewPanel>
    </Screen>
  </Page>
</PCF>