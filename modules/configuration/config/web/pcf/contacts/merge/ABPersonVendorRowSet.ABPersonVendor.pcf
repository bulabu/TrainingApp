<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../../../pcf.xsd">
  <RowSet
    id="ABPersonVendorRowSet"
    mode="ABPersonVendor|ABAttorney|ABDoctor">
    <Require
      name="contact"
      type="ABPerson"/>
    <Require
      name="duplicate"
      type="ABPerson"/>
    <Require
      name="merged"
      type="ABPerson"/>
    <Variable
      initialValue="{contact, duplicate, merged}"
      name="contacts"
      type="ABPerson[]"/>
    <Row>
      <Cell
        bold="true"
        colspan="4"
        id="contactInfo"
        value="displaykey.Web.Contacts.ReviewDuplicateContacts.ContactInfo"/>
    </Row>
    <Row>
      <Cell
        id="preferredVendorLabel"
        value="displaykey.Web.ContactDetail.AdditionalInfo.IsPreferredVendor"/>
      <CellIterator
        elementName="aContact"
        id="preferredVendor"
        value="contacts">
        <Cell
          editable="aContact == merged and perm.ABContact.createpreferred"
          id="preferredVendor"
          value="(aContact as ABPersonVendor).Preferred"/>
      </CellIterator>
    </Row>
    <Row>
      <Cell
        bold="true"
        colspan="4"
        id="taxInfo"
        value="displaykey.Web.Contacts.ReviewDuplicateContacts.TaxInfo"/>
    </Row>
    <Row>
      <Cell
        id="w9ReceivedLabel"
        value="displaykey.Web.ContactDetail.TaxInfo.W9received"/>
      <CellIterator
        elementName="aContact"
        id="w9Received"
        value="contacts">
        <Cell
          editable="aContact == merged"
          id="w9Received"
          value="aContact.W9Received"/>
      </CellIterator>
    </Row>
    <Row>
      <Cell
        id="w9ReceivedDateLabel"
        value="displaykey.Web.ContactDetail.TaxInfo.W9receivedDate"/>
      <CellIterator
        elementName="aContact"
        id="w9ReceivedDate"
        value="contacts">
        <DateCell
          editable="aContact == merged"
          id="w9ReceivedDate"
          value="aContact.W9ReceivedDate"/>
      </CellIterator>
    </Row>
    <Row>
      <Cell
        id="w9ValidFromLabel"
        value="displaykey.Web.ContactDetail.TaxInfo.W9ValidFrom"/>
      <CellIterator
        elementName="aContact"
        id="w9ValidFromDate"
        value="contacts">
        <DateCell
          editable="aContact == merged"
          id="w9ValidFrom"
          value="aContact.W9ValidFrom"/>
      </CellIterator>
    </Row>
    <Row>
      <Cell
        id="w9ValidToLabel"
        value="displaykey.Web.ContactDetail.TaxInfo.W9ValidTo"/>
      <CellIterator
        elementName="aContact"
        id="w9ValidToDate"
        value="contacts">
        <DateCell
          editable="aContact == merged"
          id="w9ValidTo"
          value="aContact.W9ValidTo"/>
      </CellIterator>
    </Row>
  </RowSet>
</PCF>